body {
    font-size: 1.25em;
    font-family: Poppins, sans-serif;
    color: hsl(240, 4%, 20%)
}

h1, h2, h3, h4 {
    color: hsl(240, 8%, 12%);
    line-height: 1.2
}

h1 {
    font-size: 2.0736em
}

h2 {
    font-size: 1.728em
}

h3 {
    font-size: 1.44em
}

h4 {
    font-size: 1.2em
}

small {
    font-size: 0.83333em
}

a, .link {
    color: hsl(216, 62%, 47%);
    text-decoration: underline
}

strong {
    font-weight: bold
}

s {
    text-decoration: line-through
}

u {
    text-decoration: underline
}

.text-component h1, .text-component h2, .text-component h3, .text-component h4 {
    line-height: 1.2;
    margin-bottom: 2.5px
}

.text-component h2, .text-component h3, .text-component h4 {
    margin-top: 7.5px
}

.text-component p, .text-component blockquote, .text-component ul li, .text-component ol li {
    line-height: 1.6
}

.text-component ul, .text-component ol, .text-component p, .text-component blockquote, .text-component .text-component__block {
    margin-bottom: 7.5px
}

.text-component ul, .text-component ol {
    list-style-position: inside
}

.text-component ul {
    list-style-type: disc
}

.text-component ol {
    list-style-type: decimal
}

.text-component img {
    display: block;
    margin: 0 auto
}

.text-component figcaption {
    text-align: center;
    margin-top: 5px
}

.text-component em {
    font-style: italic
}

.text-component hr {
    margin-top: 10px;
    margin-bottom: 10px;
    margin-left: auto;
    margin-right: auto
}

.text-component > *:first-child {
    margin-top: 0
}

.text-component > *:last-child {
    margin-bottom: 0
}

.text-component__block--full-width {
    width: 100vw;
    margin-left: calc(50% - 50vw)
}

@media (min-width: 48rem) {
    .text-component__block--left, .text-component__block--right {
        width: 45%
    }

    .text-component__block--left img, .text-component__block--right img {
        width: 100%
    }

    .text-component__block--left {
        float: left;
        margin-right: 7.5px
    }

    .text-component__block--right {
        float: right;
        margin-left: 7.5px
    }
}

@media (min-width: 90rem) {
    .text-component__block--outset {
        width: calc(100% + 105px)
    }

    .text-component__block--outset img {
        width: 100%
    }

    .text-component__block--outset:not(.text-component__block--right) {
        margin-left: -52.5px
    }

    .text-component__block--left, .text-component__block--right {
        width: 50%
    }

    .text-component__block--right.text-component__block--outset {
        margin-right: -52.5px
    }
}

.icon {
    display: inline-block;
    color: inherit;
    fill: currentColor;
    height: 1em;
    width: 1em;
    line-height: 1;
    flex-shrink: 0;
    max-width: initial
}

.icon--xxxs {
    width: 8px;
    height: 8px
}

.icon--xxs {
    width: 12px;
    height: 12px
}

.icon--xs {
    width: 16px;
    height: 16px
}

.icon--sm {
    width: 24px;
    height: 24px
}

.icon--md {
    width: 32px;
    height: 32px
}

.icon--lg {
    width: 48px;
    height: 48px
}

.icon--xl {
    width: 64px;
    height: 64px
}

.icon--xxl {
    width: 96px;
    height: 96px
}

.icon--xxxl {
    width: 128px;
    height: 128px
}

.icon--is-spinning {
    -webkit-animation: icon-spin 1s infinite linear;
    animation: icon-spin 1s infinite linear
}

@-webkit-keyframes icon-spin {
    0% {
        transform: rotate(0deg)
    }
    100% {
        transform: rotate(360deg)
    }
}

@keyframes icon-spin {
    0% {
        transform: rotate(0deg)
    }
    100% {
        transform: rotate(360deg)
    }
}

.icon use {
    color: inherit;
    fill: currentColor
}

.btn {
    position: relative;
    display: inline-flex;
    justify-content: center;
    align-items: center;
    white-space: nowrap;
    text-decoration: none;
    line-height: 1;
    font-size: 1em;
    padding-top: 3.75px;
    padding-bottom: 3.75px;
    padding-left: 12.5px;
    padding-right: 12.5px;
    border-radius: 0
}

.btn--sm {
    font-size: 0.8em
}

.btn--md {
    font-size: 1.2em
}

.btn--lg {
    font-size: 1.4em
}

.btn--icon {
    padding: 3.75px
}

.form-control {
